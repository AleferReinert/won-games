import type { ReactElement } from 'react'
import {{template}}Template from 'templates/{{template}}/{{template}}'
import * as S from './{{dashCase name}}.styles'

const {{pascalCase name}}Page = () =>  {
  return (
    <S.Wrapper>
    {{name}}
    </S.Wrapper>
  )
}

{{pascalCase name}}Page.getLayout = function getLayout(page: ReactElement) {
  return <{{template}}Template>{page}</{{template}}Template>
}

export default {{pascalCase name}}Page
